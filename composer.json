{
    "name": "saturion/yii2-mfa",
    "description": "MFA-Auth for yii2, based on vxm/yii2-mfa fork",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","mfa"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Christof Opresnik",
            "email": "christof.opresnik@outlook.com"
        }
    ],
    "require": {
        "php": ">= 8",
        "yiisoft/yii2": ">=2",
        "spomky-labs/otphp": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "vxm\\mfa\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
